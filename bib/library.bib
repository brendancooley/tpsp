@article{Krainin2016,
abstract = {We present a dynamic, complete-information bargaining model of war with N countries. The analysis of a network with more than two countries significantly generalizes previous work. We consider an environment where a country's military strength varies stochastically over time as a function of endogenous consumption. If countries are sufficiently patient, then war is inevitable and will quickly spread throughout the network. We leverage insights from the limiting case to identify two key factors that promote peace among countries with limited patience: peace is easier to sustain when countries have equal military strength or when mechanisms exist to preserve any existing inequalities. Varying the number of countries does not have a consistent effect. A n important challenge for international relations the-orists is to understand what determines the stability of a system. Different international systems have experienced vastly different levels of conflict. The Congress of Vienna and the Cold War systems were markedly peace-ful compared to the systems that followed the Treaties of Utrecht (1713) and Rastatt (1714) and, even more disas-trously, the Treaty of Versailles (1919). Today, the Middle East experiences near-constant conflict, while other regions full of potential foes, like East Asia, remain tense but peace-ful. Why does a stable equilibrium (or peaceful balance of power) exist in one system but not in another? Wagner (2007) suggests that international systems are best understood in a bargaining model, where countries ne-gotiate how to share current and future resources. If the countries fail to agree on a peaceful split, war results. Thus, Wagner argues, the threat of war is the fundamental de-terminant of outcomes in a system. Previous work (includ-ing Fearon 1995; Leventoglu and Slantchev 2007; Powell 1999, 2004a, 2006; Wagner 2000) has developed models based on that framework for the case of two countries, but this article presents the first dynamic, N-country bargaining model of war. 1 (Alesina and Spolaore [2003, 2005] explore the static division of resources in a multicountry model without a mi-crofoundation of bargaining under the threat of war. Krainin [2014] considers a static, multicountry bargaining model.) In our model, the distribution of military power across coun-tries shifts stochastically over time as a function of endoge-nous consumption. These dynamic changes in power and the commitment problems that they engender are the cause of instability and war in our model. Countries cannot commit to future divisions of resources because there is no outside entity to enforce contracts. Therefore, as countries rise and fall in strength, a newly strong country cannot resist using its in-creased bargaining power to renegotiate any previous agree-ment to its own benefit. Anticipating that behavior, an ini-tially strong country may start a war in order to lock in its current advantage. The main result of our article is that when countries are sufficiently patient, then war is inevitable, and once it starts it will spread quickly throughout the network of countries‚Äîno international system is stable except the case of a single country. The details of our baseline model are as follows: in every period, N ‚â• 2 countries divide and consume a fixed amount of resources. Each country has a level of military strength that varies over time according to a stochastic transition function that depends (as in Fearon 1996; Powell 2006, 185‚Äì 88) on that current consumption. In any period, a country can reject the peaceful division of resources and start a war against any country or countries with which it is connected},
author = {Krainin, Colin and Wiseman, Thomas},
file = {:Users/brendancooley/Library/Application Support/Mendeley Desktop/Downloaded/Krainin, Wiseman - Unknown - War and Stability in Dynamic International Systems.pdf:pdf},
issn = {0022-3816},
journal = {The Journal of Politics},
number = {4},
pages = {000--000},
title = {{War and Stability in Dynamic International Systems}},
volume = {78},
year = {2016}
}
