volume = {72},
year = {2018}
}
@techreport{deSoysa2010,
abstract = {It is widely believed that nations fight over oil. Yet, available evidence is largely anecdotal. We offer the first systematic analysis of the "blood oil" thesis, while at the same time presenting an alternative conception of the relationship between petroleum and international politics. Precisely because oil wars are likely to disrupt the world economy, producers and major consuming nations have intense incentives to seek to deter such contests. In contrast to the conventional wisdom, widespread recognition of the hazard of oil wars actually serves to enhance the territorial integrity of petroleum exporters. Heightened security in turn creates a moral hazard in which oil states have a freer hand to act aggressively abroad. We find no evidence that petroleum exporters are more likely to be attacked. Instead, oil exporters appear better able to flout global norms of non-aggression, more often using force against non-petroleum exporters.},
author = {{De Soysa}, Indra and Gartzke, Erik and Lie, Tove Grete},
