abstract = {While much research in political economy points out the benefits of ‚Äúlimited government,‚Äù political scientists have long emphasized the problems created in many less-developed nations by ‚Äúweak states,‚Äù which lack the power to tax and regulate the economy and to withstand the political and social challenges from non-state actors. I construct a model in which the state apparatus is controlled by a self-interested ruler, who tries to divert resources for his own consumption, but who can also invest in socially productive public goods. Both weak and strong states create distortions. When the state is excessively strong, the ruler imposes such high taxes that economic activity is stifled. When the state is excessively weak, the ruler anticipates that he will not be able to extract rents in the future and underinvests in public goods. I show that the same conclusion applies in the analysis of both the economic power of the state (i.e., its ability to raise taxes) and its political power (i.e., its ability to remain entrenched from the citizens). I also discuss how under certain circumstances a different type of equilibrium, which I refer to as ‚Äúconsensually strong state equilibrium,‚Äù can emerge whereby the state is politically weak but is allowed to impose high taxes as long as a sufficient fraction of the proceeds are invested in public goods. The consensually strong state might best correspond to the state in OECD countries where taxes are high despite significant control by the society over the government.},
author = {Acemoglu, Daron},
file = {:Users/brendancooley/Library/Application Support/Mendeley Desktop/Downloaded/Acemoglu - 2005 - Politics and economics in weak and strong states.pdf:pdf},
journal = {Journal of Monetary Economics},
month = {oct},
number = {7},
pages = {1199--1226},
title = {{Politics and economics in weak and strong states}},
volume = {52},
year = {2005}
}
