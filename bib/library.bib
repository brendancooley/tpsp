@article{Becker2006,
abstract = {This paper considers the costs of reducing consumption of a good by making its production illegal and punishing apprehended illegal pro-ducers. We use illegal drugs as a prominent example. We show that the more inelastic either demand for or supply of a good is, the greater the increase in social cost from further reducing its production by greater enforcement efforts. So optimal public expenditures on ap-prehension and conviction of illegal suppliers depend not only on the difference between the social and private values from consumption but also on these elasticities. When demand and supply are not too elastic, it does not pay to enforce any prohibition unless the social value is negative. We also show that a monetary tax could cause a greater reduction in output and increase in price than optimal en-forcement against the same good would if it were illegal, even though some producers may go underground to avoid a monetary tax. When enforcement is costly, excise taxes and quantity restrictions are not equivalent.},
author = {Becker, Gary S and Murphy, Kevin M and Grossman, Michael},
