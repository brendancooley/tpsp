@techreport{SanchezdelaSierra2018,
abstract = {A positive demand shock for coltan, a mineral whose bulky output is difficult to conceal, leads armed actors in eastern Congo to create illicit customs taxation at coltan trading points. There, they emerge as "stationary bandits" providing protection. A similar shock for gold, whose hard-to-find valuable output can be concealed from customs' inspections, leads armed actors to require working permits to work at gold mines, and to introduce an illicit tax on food sales in the villages where wealth from gold is in circulation. Rather than at the mines, "stationary bandits" providing protection emerge in these villages, where they also create illicit fiscal and judicial administrations to increase revenues from sales taxes, poll taxes, and tolls. Incidentally, if population well-being is part of the stationary bandit's objective, having a stationary bandit increases household welfare. The findings suggest that, irrespective of their stated goals, armed actors may create, intensify, or disinvest in the "essential functions of a state" to better expropriate. In some cases, these functions can enable economic growth.},
author = {{S{\'{a}}nchez De La Sierra}, Ra{\'{u}}l},
file = {:Users/brendancooley/Library/Application Support/Mendeley Desktop/Downloaded/S{\'{a}}nchez De La Sierra - 2018 - On the Origins of the State Stationary Bandits and Taxation in Eastern Congo.pdf:pdf},
title = {{On the Origins of the State: Stationary Bandits and Taxation in Eastern Congo}},
year = {2018}
}
@article{Goodliffe2009,
abstract = {The first proposal for the International Criminal Court (ICC) in 1994 envisioned a weak institution. Over the subsequent four years, states surprisingly strengthened the draft to create a robust ICC with novel enforcement authority. What happened and why? We argue that during negotiations governments adopted the positions of the international partners on whom they depend for a diverse set of goods that includes trade, security, and foreign policy success in international organizations. We label this set of partners a dependence network. In our approach, leaders watch closely how other governments behave within their dependence network and alter their own actions accordingly. We test this theory against a variety of other explanations on a new database that codes state negotiating positions relative to four key institutional features of the ICC. We find that trade networks substantially influence state negotiating positions on the ICC even taking into account an array of other factors. {\textcopyright} 2009 Southern Political Science Association.},
author = {Goodliffe, Jay and Hawkins, Darren},
doi = {10.1017/S0022381609090835},
file = {:Users/brendancooley/Library/Application Support/Mendeley Desktop/Downloaded/Goodliffe, Hawkins - 2009 - A Funny Thing Happened on the Way to Rome Explaining International Criminal Court Negotiations.pdf:pdf},
issn = {0022-3816},
