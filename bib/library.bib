author = {Jin, Jiashun},
file = {:Users/brendancooley/Library/Application Support/Mendeley Desktop/Downloaded/Jin - 2015 - FAST COMMUNITY DETECTION BY SCORE.pdf:pdf},
journal = {The Annals of Statistics},
keywords = {62H30,62P25,91C20,Community detection,Degree Corrected Block Model (DCBM),Hamming distance,PCA,k-means method,moderate deviation,modularity,social network,sparsity,spectral analysis},
number = {1},
pages = {57--89},
title = {{Fast Community Detection by SCORE}},
volume = {43},
year = {2015}
}
@article{Steinberg2014,
abstract = {Conventional wisdom holds that autocracies are more likely than democracies to adopt interventionist and protectionist economic policies, including fixed and undervalued exchange rates. This article suggests that this view is only partially correct: nondemocracies are a heterogeneous grouping, and only some types of authoritarian regimes adopt different foreign economic policies from those of their democratic counterparts. Using the example of exchange rate policy, the authors show that foreign economic policy varies across monarchic, military, and civilian dictatorships. More specifically, they hypothesize that monarchies and military regimes are more likely than democracies and civilian dictatorships to maintain fixed exchange rate regimes because the former regimes have smaller ‚Äúselectorates‚Äù than the latter. The authors also expect that monarchies and civilian dictatorships maintain more undervalued exchange rates than democracies and military regimes because the former regimes provide their leaders with greater tenure security than the latter regimes. These hypotheses are evaluated using a time-series‚Äìcross-sectional data set of a large sample of developing countries from 1973 to 2006. The statistical results accord with these predictions. These findings indicate that the ways in which democracies engage with the global economy may be less unique than many believe.},
