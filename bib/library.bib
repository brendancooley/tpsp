@article{Diehl1985,
abstract = {This research note analyzes the importance of geography for the study of conflict and explores the question: Does the geographic location (relative to the nations involved) of a confrontation influence the likelihood that the confrontation will escalate to war? It was discovered that if a dispute is contiguous by land to one of the disputants, the likelihood of escalation is increased (and increased even more if both sides are contiguous to the site). Ninety-two percent of the wars in the sample began with a dispute that was contiguous to one or both disputants; only two percent of the noncontiguous disputes escalated to war. Various interpretations of the results and suggestions for future research are noted. In the formative years of international relations as a discipline, scholars devoted a great deal of attention to geography. The theoretical approaches of Mahan (1918), Mackinder (1919), and Spykman (1944) first suggested that a nation's geographic location or control over specified areas of the globe was critical in understanding conflict. As international relations matured as a subfield, debates focused on world views, methodological approaches, and power, leaving the importance of geography to be considered by historians and military geographers. Geographic factors, however, have not been completely ignored in the study of conflict. In his classic study, Statistics of Deadly Quarrels, Lewis F. Richardson (1960) pointed out that the number of frontiers a nation had was directly related to the number of external wars in which it participated.' Starr and Most (1978) qualified Richardson's conclusions by demonstrating that a large number of colonial frontiers increased the likelihood of war, while a large number of home frontiers had the opposite effect.2 Other recent work (Most and Starr, 1980; Bremer, 1982; Starr and Most, 1983) has examined the "diffusion hypothesis," which states that conflict within a nation increases the probability that its neighbors will also become involved in conflict. 'The author would like to thank Steven Elliott, Loch Johnson, and four anonymous reviewers for their valuable assistance and comments.},
author = {Diehl, Paul F},
file = {:Users/brendancooley/Library/Application Support/Mendeley Desktop/Downloaded/Diehl - 1985 - Continuity and Military Escalation in Major Power Rivalries, 1816-1980.pdf:pdf},
journal = {The Journal of Politics},
number = {4},
pages = {1203--1211},
title = {{Continuity and Military Escalation in Major Power Rivalries, 1816-1980}},
volume = {47},
year = {1985}
}
