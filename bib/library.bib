number = {03},
pages = {977},
publisher = {Cambridge University PressNew York, USA},
title = {{A Funny Thing Happened on the Way to Rome: Explaining International Criminal Court Negotiations}},
url = {http://www.journals.uchicago.edu/doi/abs/10.1017/S0022381609090835 http://www.journals.cambridge.org/abstract{\_}S0022381609090835},
