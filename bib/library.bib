file = {:Users/brendancooley/Library/Application Support/Mendeley Desktop/Downloaded/Waugh - 2010 - International Trade and Income Differences(2).pdf:pdf},
journal = {American Economic Review},
pages = {2093--2124},
title = {{International Trade and Income Differences}},
volume = {100},
year = {2010}
