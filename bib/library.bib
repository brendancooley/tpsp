author = {Chaney, Thomas},
file = {:Users/brendancooley/Library/Application Support/Mendeley Desktop/Downloaded/Unknown - 2008 - Distorted Gravity The Intensive and Extensive Margins of International Trade.pdf:pdf},
journal = {American Economic Review},
